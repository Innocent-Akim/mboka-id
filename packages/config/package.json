{
  "name": "@mboka-id/config",
  "version": "0.0.1",
  "description": "Centralized configuration package for Mboka ID applications",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint \"src/**/*.ts\" --fix",
    "type-check": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@nestjs/config": "^4.0.2",
    "@nestjs/typeorm": "^11.0.0",
    "joi": "^17.13.3",
    "pg": "^8.16.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/typeorm": "^11.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "@types/pg": "^8.11.10",
    "typescript": "^5.7.3"
  }
}
